<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:ex="http://example.org/ontology#">

  <!-- Déclaration de l'ontologie -->
  <owl:Ontology rdf:about="http://example.org/ontology">
    <rdfs:comment>Une ontologie d'exemple pour la gestion des connaissances.</rdfs:comment>
  </owl:Ontology>

  <!-- Définition des Classes OWL -->
  <owl:Class rdf:about="http://example.org/ontology#KnowledgeDomain">
    <rdfs:label>Knowledge Domain</rdfs:label>
    <rdfs:label>Domaine de la connaissance</rdfs:label>
   <rdfs:label>Gestion des connaissances</rdfs:label>   
    <rdfs:comment>Un domaine de connaissance, par exemple, informatique, biologie, etc.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/ontology#Document">
    <rdfs:label>Document</rdfs:label>
    <rdfs:comment>Un document contenant des informations, comme un article ou un rapport.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Un utilisateur du système de gestion de connaissance.</rdfs:comment>
  </owl:Class>

  <!-- Sous-classes (rdfs:subClassOf) -->
  <rdf:Description rdf:about="http://example.org/ontology#ResearchPaper">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>Research Paper</rdfs:label>
    <rdfs:comment>Un document de type article de recherche.</rdfs:comment>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ontology#Thesis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>Thesis</rdfs:label>
    <rdfs:comment>Une thèse académique.</rdfs:comment>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ontology#AcademicUser">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Academic User</rdfs:label>
    <rdfs:comment>Un utilisateur académique, par exemple, un étudiant ou un professeur.</rdfs:comment>
  </rdf:Description>

  <!-- Définition des Propriétés OWL -->
  <owl:ObjectProperty rdf:about="http://example.org/ontology#hasDocument">
    <rdfs:domain rdf:resource="http://example.org/ontology#KnowledgeDomain"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>has Document</rdfs:label>
    <rdfs:comment>Indique qu'un domaine de connaissance contient un document.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/ontology#hasResearchPaper">
    <rdfs:subPropertyOf rdf:resource="http://example.org/ontology#hasDocument"/>
    <rdfs:domain rdf:resource="http://example.org/ontology#KnowledgeDomain"/>
    <rdfs:range rdf:resource="http://example.org/ontology#ResearchPaper"/>
    <rdfs:label>has Research Paper</rdfs:label>
    <rdfs:comment>Indique qu'un domaine de connaissance contient un article de recherche.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/ontology#hasUser">
    <rdfs:domain rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>has User</rdfs:label>
    <rdfs:comment>Indique qu'un document est associé à un utilisateur.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/ontology#hasAcademicUser">
    <rdfs:subPropertyOf rdf:resource="http://example.org/ontology#hasUser"/>
    <rdfs:domain rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/ontology#AcademicUser"/>
    <rdfs:label>has Academic User</rdfs:label>
    <rdfs:comment>Indique qu'un document est associé à un utilisateur académique.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/ontology#isRelatedTo">
    <rdfs:domain rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>is Related To</rdfs:label>
    <rdfs:comment>Indique qu'un document est lié à un autre document.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/ontology#isCitedBy">
    <rdfs:subPropertyOf rdf:resource="http://example.org/ontology#isRelatedTo"/>
    <rdfs:domain rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>is Cited By</rdfs:label>
    <rdfs:comment>Indique qu'un document est cité par un autre document.</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Définition des Individus OWL -->
  <owl:NamedIndividual rdf:about="http://example.org/ontology#ComputerScience">
    <rdf:type rdf:resource="http://example.org/ontology#KnowledgeDomain"/>
    <rdfs:label>Computer Science</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/ontology#Mathematics">
    <rdf:type rdf:resource="http://example.org/ontology#KnowledgeDomain"/>
    <rdfs:label>Mathematics</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/ontology#AIResearch">
    <rdf:type rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>AI Research Paper</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/ontology#Philosophy101">
    <rdf:type rdf:resource="http://example.org/ontology#Document"/>
    <rdfs:label>Philosophy 101</rdfs:label>
  </owl:NamedIndividual>

  <!-- Association des Individus avec des Propriétés -->
  <rdf:Description rdf:about="http://example.org/ontology#AIResearch">
    <ex:hasAcademicUser rdf:resource="http://example.org/ontology#JohnDoe"/>
    <ex:isCitedBy rdf:resource="http://example.org/ontology#Philosophy101"/>
  </rdf:Description>

  <owl:NamedIndividual rdf:about="http://example.org/ontology#JohnDoe">
    <rdf:type rdf:resource="http://example.org/ontology#AcademicUser"/>
    <rdfs:label>John Doe</rdfs:label>
  </owl:NamedIndividual>

</rdf:RDF>
